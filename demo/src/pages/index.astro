---
import Layout from 'astro-travelens/layout'
import Header from 'astro-travelens/header'
import Tags from 'astro-travelens/tags'
import Footer from 'astro-travelens/footer'
import Map from 'astro-travelens/map'

import { SiteMetadata } from 'astro-travelens/config'
import Gallery from '../components/Gallery.astro'
---

<Layout title={SiteMetadata.title}>
	<Header />
	<Tags tags={["a", "b", "c"]} />
	<Gallery folder="devils-corner" />
	<Map text="Devil's Corner" lat={-41.97224} lng={148.15147} zoom={17} />
	<Footer />
</Layout>
